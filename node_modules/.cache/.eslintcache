const [filterByID, setFilterByID] = useState(0);\n  const [filterByType, setFilterByType] = useState('all');\n  const [filterBySupplier, setFilterBySupplier] = useState('all');\n\n  const types = [...stocks].map(stock => stock.type);\n  const suppliers = [...stocks].map(stock => stock.supplier);\n\n\n  useEffect(() => {\n    fetch('http://localhost:3000/materials')\n    .then(r => r.json())\n    .then(setStocks)\n    .catch(err => alert(err.message))\n  }, []);\n\n\n  const handleQuantitySubmit = (e, targetStock) => {\n    e.preventDefault();\n\n    const newStocks = [...stocks].map(stock => stock.id === targetStock.id ? {...stock, quantity: newQuantity} : stock); \n\n    fetch(`http://localhost:3000/materials/${targetStock.id}`, {\n        method: \"PATCH\",\n        headers: {'Content-type': 'application/json'},\n        body: JSON.stringify({quantity: newQuantity})\n    })\n    .then(r => r.json())\n    .then(setStocks(newStocks))\n    .catch(err => alert(err.message))\n  };\n\n\n  const filteredStocks = [...stocks].filter(stock => {if (filterByID === stock.id || filterByID === 0) return true})\n                                    .filter(stock => {if (filterByType === stock.type || filterByType === 'all') return true})\n                                    .filter(stock => {if (filterBySupplier === stock.supplier || filterBySupplier === 'all') return true})\n\n\n    return (\n      <div>\n        <SearchFilterBar types={types} suppliers={suppliers} setFilterByID={setFilterByID} setFilterByType={setFilterByType} setFilterBySupplier={setFilterBySupplier} />\n        <StockDisplayer filteredStocks={filteredStocks} setNewQuantity={setNewQuantity} handleQuantitySubmit={handleQuantitySubmit} />\n      </div>\n    )\n};","/Users/jk/Desktop/Flatiron School/Development/code/phases/phase-2/fisphase2-project/src/components/Contact.js",[],[],"/Users/jk/Desktop/Flatiron School/Development/code/phases/phase-2/fisphase2-project/src/components/NavBar.js",[],[],"/Users/jk/Desktop/Flatiron School/Development/code/phases/phase-2/fisphase2-project/src/components/StockDisplayer.js",[],[],"/Users/jk/Desktop/Flatiron School/Development/code/phases/phase-2/fisphase2-project/src/components/sub_component/ViewStockDetail.js",["80"],[],"import { useState, useEffect } from 'react';\nimport { useNavigate, useParams } from 'react-router-dom';\n\nexport default function ViewStockDetail() {\n    document.title = 'Stock Detail'\n\n    const navigateTo = useNavigate();\n    const {stockID} = useParams();\n    const [stock, setStock] = useState([]);\n    \n    useEffect(() => {\n        fetch(`http://localhost:3000/materials/${stockID}`)\n        .then(r => r.json())\n        .then(setStock)\n        .catch(err => alert(err.message))\n    }, [])\n\n    return (\n        <div className='display-component'>\n            <input type='button' onClick={() => navigateTo('/')} value='Back to Dashboard' />\n\n            <div className='stock-detail'>\n                <p>{stock.atname} ({stock.type})</p>\n                <img src={stock.imgURL} alt={stock.name} width={'200px'}/>\n                <p>Stock# {stock.id}</p>\n                <p>Supplier: {stock.supplier}</p>\n                <p>{stock.note}</p>\n            </div>\n        </div>\n    );\n}",[],"/Users/jk/Desktop/Flatiron School/Development/code/phases/phase-2/fisphase2-project/src/components/sub_component/StockManagerSelector.js",[],[],"/Users/jk/Desktop/Flatiron School/Development/code/phases/phase-2/fisphase2-project/src/components/sub_component/RegisterForm.js",[],[],"/Users/jk/Desktop/Flatiron School/Development/code/phases/phase-2/fisphase2-project/src/components/sub_component/EditorForm.js",[],[],"/Users/jk/Desktop/Flatiron School/Development/code/phases/phase-2/fisphase2-project/src/components/sub_component/DeleteForm.js",[],[],"/Users/jk/Desktop/Flatiron School/Development/code/phases/phase-2/fisphase2-project/src/components/sub_component/SearchFilterBar.js",["81","82"],[],"/Users/jk/Desktop/Flat