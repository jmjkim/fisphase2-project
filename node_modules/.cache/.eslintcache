/fisphase2-project-database.herokuapp.com/materials/${targetStock.id}`, {\n        method: \"PATCH\",\n        headers: {'Content-type': 'application/json'},\n        body: JSON.stringify({quantity: newQuantity})\n    })\n    .then(r => r.json())\n    .then(setStocks(newStocks))\n    .catch(err => alert(err.message))\n  };\n\n\n  const filteredStocks = [...stocks].filter(stock => {if (filterByID === stock.id || filterByID === 0) return true})\n                                    .filter(stock => {if (filterByType === stock.type || filterByType === 'all') return true})\n                                    .filter(stock => {if (filterBySupplier === stock.supplier || filterBySupplier === 'all') return true})\n\n\n    return (\n      <div>\n          <SearchFilterBar types={types} \n                           suppliers={suppliers}\n                           setFilterByID={setFilterByID} \n                           setFilterByType={setFilterByType} \n                           setFilterBySupplier={setFilterBySupplier} />\n\n\n          <StockDisplayer filteredStocks={filteredStocks} \n                          setNewQuantity={setNewQuantity} \n                          handleQuantitySubmit={handleQuantitySubmit} />\n      </div>\n    );\n};\n\nexport default Dashboard;","/Users/jk/Desktop/Flatiron School/Development/code/phases/phase-2/fisphase2-project/src/components/Contact.js",[],[],"/Users/jk/Desktop/Flatiron School/Development/code/phases/phase-2/fisphase2-project/src/components/NavBar.js",[],[],"/Users/jk/Desktop/Flatiron School/Development/code/phases/phase-2/fisphase2-project/src/components/StockDisplayer.js",["99"],[],"import React from 'react';\nimport { Link } from 'react-router-dom';\n\n\nconst StockDisplayer = ({ filteredStocks, setNewQuantity, handleQuantitySubmit }) => {\n    return (\n        <>\n            {filteredStocks.map((stock, key) => {\n                const {id, imgURL, name, type, quantity, supplier, note} = stock;\n\n\n                <React.Fragment key={key}>\n                  <h3>List of Stocks</h3>\n                \n\n                  <table>\n                      <thead className='table-header'>\n                          <tr>\n                              <td>ID</td>\n                              <td>Image</td>\n                              <td>Name</td>\n                              <td>Type</td>\n                              <td>Quantity (max 100)</td>\n                              <td>Supplier</td>\n                              <td>In-stock</td>\n                              <td>Note</td>\n                          </tr>\n                      </thead>\n\n\n                      <tbody>\n                        <tr className='stock-table'>\n                            <td>{id}</td>\n                            <td>\n                                <Link to={`viewstockdetail/${id}`}>\n                                    <img title='Click to view detail' src={imgURL} alt={name} width={'50px'} style={{cursor: 'pointer'}} />\n                                </Link>\n                            </td>\n                            <td>{name}</td>\n                            <td>{type}</td>\n                            <td>\n                                Current: {quantity <= 5 ? <b style={{color : 'rgb(250, 0, 0)'}}>{quantity}</b> : <b>{quantity}</b>}\n\n                                <form className='stock-row-quantity-form' onSubmit={e => handleQuantitySubmit(e, stock)}>\n                                    <label>New Quantity: </label>\n\n                                    <input type='number' name='quantity' min={0} max={100} onChange={e => setNewQuantity(+e.target.value)} />\n                                    <input type='submit' value='Update' />\n                                </form>\n                            </td>\n                            <td>{supplier}</td>\n                            <td>{quantity > 0 ? <b>Yes</b> : <b style={{color : 'rgb(250, 0, 0)'}}>No</b>}</td>\n                            <td>{note}</td>\n                        </tr>\n                      </tbody>\n\n\n                  </table>\n                </React.Fragment>\n            })}\n        </>\n    );\n}\n\nexport default StockDisplayer;",[],"/Users/jk/Desktop/Flatiron School/Development/code/phases/phase-2/fisphase2-project/src/components/sub_component/ViewStockDetail.js",["100"],[],"import { useState, useEffect } from 'react';\nimport { useNavigate, useParams } from 'react-router-dom';\n\nconst ViewStockDetail = () => {\n    document.title = 'Stock Detail'\n\n    const [stock, setStock] = useState([]);\n    const navigateTo = useNavigate();\n    const {stockID} = useParams();\n    \n\n    useEffect(() => {\n        fetch(`https://fisphase2-project-database.herokuapp.com/materials/${stockID}`)\n        .then(r => r.json())\n        .then(setStock)\n        .catch(err => alert(err.message))\n    }, [])\n\n    \n    return (\n        <>\n            <div className='stock-detail'>\n                <h3>Stock Detail</h3>\n\n                <p>Stock# {stock.id} {stock.name} ({stock.type})</p>\n                <img src={stock.imgURL} alt={stock.name} width={'100px'}/>\n                <p>Supplier: {stock.supplier}</p>\n                <p>{stock.note}</p>\n                \n                <input type='button' onClick={() => navigateTo('/')} value='Back to Dashboard' />\n            </div>\n        </>\n    );\n}\n\nexport default ViewStockDetail;",[],"/Users/jk/Desktop/Flatiron School/Development/code/phases/phase-2/fisphase2-project/src/components/sub_component/StockManagerSelector.js",[],[],"/Users/jk/Desktop/Flatiron School/Development/code/phases/phase-2/fisphase2-project/src/components/sub_component/SearchFilterBar.js",["101","102"],[],"const SearchFilterBar = ({ types, suppliers, setFilterByID, setFilterByType, setFilterBySupplier }) => {\n    return (\n        <div className='search-filter-bar'>\n            <label>Search By: </label>\n           