s/jk/Desktop/Flatiron School/Development/code/phases/phase-2/fisphase2-project/src/components/Dashboard.js",["82","83","84"],[],"import { useState, useEffect } from 'react';\n\nimport SearchFilterBar from './sub_component/SearchFilterBar';\nimport StockDisplayer from './StockDisplayer';\n\nconst Dashboard = () => {\n  document.title = 'Inventory Manager';\n\n  const [stocks, setStocks] = useState([]);\n  const [newQuantity, setNewQuantity] = useState(0);\n  const [filterByID, setFilterByID] = useState(0);\n  const [filterByType, setFilterByType] = useState('all');\n  const [filterBySupplier, setFilterBySupplier] = useState('all');\n\n  const types = [...stocks].map(stock => stock.type);\n  const suppliers = [...stocks].map(stock => stock.supplier);\n\n\n  useEffect(() => {\n    fetch('http://localhost:3000/materials')\n    .then(r => r.json())\n    .then(setStocks)\n    .catch(err => alert(err.message))\n  }, []);\n\n\n  const handleQuantitySubmit = (e, targetStock) => {\n    e.preventDefault();\n\n    const newStocks = [...stocks].map(stock => stock.id === targetStock.id ? {...stock, quantity: newQuantity} : stock); \n\n    fetch(`http://localhost:3000/materials/${targetStock.id}`, {\n        method: \"PATCH\",\n        headers: {'Content-type': 'application/json'},\n        body: JSON.stringify({quantity: newQuantity})\n    })\n    .then(r => r.json())\n    .then(setStocks(newStocks))\n    .catch(err => alert(err.message))\n  };\n\n\n  const filteredStocks = [...stocks].filter(stock => {if (filterByID === stock.id || filterByID === 0) return true})\n                                    .filter(stock => {if (filterByType === stock.type || filterByType === 'all') return true})\n                                    .filter(stock => {if (filterBySupplier === stock.supplier || filterBySupplier === 'all') return true})\n\n\n    return (\n      <div>\n        <SearchFilterBar types={types} suppliers={suppliers} setFilterByID={setFilterByID} setFilterByType={setFilterByType} setFilterBySupplier={setFilterBySupplier} />\n        <StockDisplayer filteredStocks={filteredStocks} setNewQuantity={setNewQuantity} handleQuantitySubmit={handleQuantitySubmit} />\n      </div>\n    )\n};\n\nexport default Dashboard;","/Users/jk/Desktop/Flatiron School/Development/code/phases/phase-2/fisphase2-project/src/components/Contact.js",[],[],"/Users/jk/Desktop/Flatiron School/Development/code/phases/phase-2/fisphase2-project/src/components/NavBar.js",[],[],"/Users/jk/Desktop/Flatiron School/Development/code/phases/phase-2/fisphase2-project/src/components/StockDisplayer.js",[],[],"/Users/jk/Desktop/Flatiron School/Development/code/phases/phase-2/fisphase2-project/src/components/sub_component/ViewStockDetail.js",["85"],[],"import { useState, useEffect } from 'react';\nimport { useNavigate, useParams } from 'react-router-dom';\n\nconst ViewStockDetail = () => {\n    document.title = 'Stock Detail'\n\n    const navigateTo = useNavigate();\n    const {stockID} = useParams();\n    const [stock, setStock] = useState([]);\n    \n    useEffect(() => {\n        fetch(`http://localhost:3000/materials/${stockID}`)\n        .then(r => r.json())\n        .then(setStock)\n        .catch(err => alert(err.message))\n    }, [])\n\n    return (\n        <div className='display-content'>\n            <div className='stock-detail'>\n                <h3>Stock Detail</h3>\n\n                <p>Stock# {stock.id} {stock.name} ({stock.type})</p>\n                <img src={stock.imgURL} alt={stock.name} width={'100px'}/>\n                <p>Supplier: {stock.supplier}</p>\n                <p>{stock.note}</p>\n                \n                <input type='button' onClick={() => navigateTo('/')} value='Back to Dashboard' />\n            </div>\n        </div>\n    );\n}\n\nexport default ViewStockDetail;","/Users/jk/Desktop/Flatiron School/Development/code/phases/phase-2/fisphase2-project/src/components/sub_component/StockManagerSelector.js",[],[],"/Users/jk/Desktop/Flatiron School/Development/code/phases/phase-2/fisphase2-project/src/components/sub_component/SearchFilterBar.js",["86","87"],[],"const SearchFilterBar = ({ types, suppliers, setFilterByID, setFilterByType, setFilterBySupplier }) => {\n    return (\n        <div className='search-filter-bar'>\n            <label>Search By: </label>\n            <input placeholder='Stock# (4 digits)' maxLength={4} onChange={(e) => setFilterByID(+e.target.value)} />    \n\n            <label> Filter By: </label>\n            <select onChange={(e) => setFilterByType(e.target.value)}>\n                <option value='all'>All</option>\n                {types.map((type, index) => {\n                    if (index === types.indexOf(type)) return <option key={index} value={type}>{type}</option>\n                })}\n            </select>\n\n            <select onChange={(e) => setFilterBySupplier(e.target.value)}>\n                <option value='all'>All</option>\n                {suppliers.map((supplier, index) => {\n                    if (index === suppliers.indexOf(supplier)) return <option key={index} value={supplier}>{supplier}</option>\n                })}\n            </select>\n        </div>\n    );\n}\n\nexport default SearchFilterBar;","/Users/jk/Desktop/Flatiron School/Development/code/phases/phase-2/fisphase2-project/src/App.js",[],[],"/Users/jk/Desktop/Flatiron School/Development/code/phases/phase-2/fisphase2-project/src/components/sub_component/StockRowDetail.js",[],[],"/Users/jk/Desktop/Flatiron School/Development/code/phases/phase-2/fisphase2-project/src/components/sub_component/StockPreview.js",[],[],"/Users/jk/Desktop/Flatiron School/Development/code/phases/phase-2/fisphase2-project/src/components/sub_component/forms/DeleteForm.js",[],[],"/Users/jk/Desktop/Flatiron School/Development/code/phases/phase-2/fisphase2-project/src/components/sub_component/forms/EditorForm.js",[],[],"/Users/jk/Desktop/Flatiron School/Development/code/phases/phase-2/fisphase2-project/src/components/sub_componen